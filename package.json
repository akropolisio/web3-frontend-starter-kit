{
  "name": "web3-frontend-starter-kit",
  "version": "0.0.1",
  "description": "Frontend starter kit for Ethereum DApps",
  "main": "index.js",
  "scripts": {
    "node": "node --max_old_space_size=4096 ./node_modules/.bin/cross-env TS_NODE_PROJECT=\"webpack/tsconfig.json\"",
    "dev": "npm run node -- webpack-dev-server --open --config webpack/dev.ts",
    "build": "npm run node -- webpack --config webpack/prod.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx && tsc --noEmit --pretty",
    "lint:autofix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand --watch",
    "test:coverage": "jest --coverage --watch",
    "codegen": "graphql-codegen --config codegen.yml; typechain --target web3-rxjs --outDir src/generated/contracts **/abi/*.json",
    "postinstall": "npm run codegen",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "author": "Serdtsev Dmitriy",
  "license": "MIT",
  "dependencies": {
    "@apollo/react-common": "^3.1.3",
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.6.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/styles": "^4.6.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-ws": "^1.0.19",
    "apollo-utilities": "^1.3.2",
    "bn.js": "^5.0.0",
    "build-route-tree": "^1.0.2",
    "classnames": "^2.2.6",
    "core-decorators": "^0.20.0",
    "final-form": "^4.18.5",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "node-polyglot": "^2.4.0",
    "notistack": "^0.9.5",
    "ramda": "^0.26.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-final-form": "^6.3.0",
    "react-number-format": "^4.3.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
